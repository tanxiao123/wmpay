<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wmpay.dao.WmCommodityDAO" >
    <select id="selectPageCommodity" resultType="wmCommodity">
        select * from wm_commodity where `status` = 1 AND user_id = #{userId} AND `type` = #{type}
    </select>
</mapper>